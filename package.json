{
  "name": "fb.me",
  "version": "0.0.0",
  "private": true,
  "description": "Lightweight Facebook Messenger SDK",
  "keywords": [
    "fb.me",
    "node",
    "typescript"
  ],
  "homepage": "https://github.com/motss/fb.me",
  "license": "MIT",
  "author": {
    "name": "Rong Sen Ng",
    "email": "wes.ngrongsen@gmail.com",
    "url": "https://github.com/motss"
  },
  "main": "./index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:motss/fb.me.git"
  },
  "scripts": {
    "build": "npm run ts && cross-env NODE_ENV=production gulp build && npm run cleanup",
    "build:debug": "npm run ts && gulp build && npm run cleanup",
    "check": "npm run lint && npm run nsp",
    "cleanup": "rm -rf gulpfile.js",
    "gulp": "npm run ts && gulp",
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "lint:fix": "tslint -p . -c tslint.json --fix",
    "nsp": "nsp check",
    "prepublishOnly": "npm run check && npm run build",
    "test": "nyc ava --verbose",
    "test:debug": "env-cmd ./src/test/.test.env nyc ava --verbose",
    "ts": "tsc --target esnext --lib esnext,esnext.asynciterable --module commonjs gulpfile.ts"
  },
  "dependencies": {
    "express": "^4.16.2",
    "make-fetch-happen": "^2.6.0",
    "whitelister": "0.0.5"
  },
  "devDependencies": {
    "@std/esm": "^0.18.0",
    "@types/express": "^4.11.0",
    "@types/node": "^8.0.57",
    "@types/node-fetch": "^1.6.7",
    "ava": "^0.24.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.2.0",
    "cross-env": "^5.1.1",
    "del": "^3.0.0",
    "env-cmd": "^7.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-sequence": "^0.4.6",
    "gulp-tslint": "^8.1.2",
    "gulp-typescript": "^3.2.3",
    "nsp": "^3.1.0",
    "nyc": "^11.4.1",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.4.2",
    "tslint-consistent-codestyle": "^1.11.0",
    "tslint-immutable": "^4.4.0",
    "typescript": "^2.6.2"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.5.1"
  },
  "@std/esm": "cjs",
  "ava": {
    "require": [
      "@std/esm"
    ],
    "files": [
      "test/index.js"
    ],
    "failFast": true
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "src/**/*.d.ts",
      "src/**/test*/*.ts",
      "src/**/test*/*.tsx",
      "src/**/demo*/*.ts",
      "src/**/demo*/*.tsx"
    ],
    "reporter": [
      "lcov",
      "text-lcov"
    ],
    "require": [
      "@std/esm"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "cache": false,
    "sourceMap": false,
    "instrument": false,
    "all": false,
    "report-dir": "./coverage",
    "watermarks": {
      "lines": [
        80,
        95
      ],
      "functions": [
        80,
        95
      ],
      "branches": [
        80,
        95
      ],
      "statements": [
        80,
        95
      ]
    }
  }
}
